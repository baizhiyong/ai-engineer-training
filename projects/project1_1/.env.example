# 环境变量配置示例文件
# 复制此文件为 .env 并填入实际的API密钥

# OpenAI API配置
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# 和风天气API配置
QWEATHER_API_KEY=your_qweather_api_key_here
QWEATHER_BASE_URL=https://devapi.qweather.com/v7

# Tavily搜索API配置
TAVILY_API_KEY=your_tavily_api_key_here

# Redis缓存配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# 应用配置
APP_NAME=MultiTaskQAAssistant
APP_VERSION=1.0.0
LOG_LEVEL=INFO
MAX_CONVERSATION_HISTORY=50
CACHE_TTL=3600