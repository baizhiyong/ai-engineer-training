# Multi Agent 客服系统环境变量配置示例
# 复制此文件为 .env 并填入实际值

# 日志配置
LOG_LEVEL=INFO

# OpenAI配置（可选）
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o

# 重试配置
MAX_RETRIES=3
RETRY_DELAY=1.0
TIMEOUT=30

# 日志配置
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_FILE=logs/app.log

# 模拟数据配置
MOCK_ORDER_DELAY=0.5
MOCK_LOGISTICS_DELAY=0.8